{
  "name": "@cybersailor/slauth-ui-vue",
  "version": "0.8.1",
  "description": "Vue 3 UI components for slauth",
  "keywords": [
    "auth",
    "aira",
    "vue",
    "vue3",
    "ui",
    "components",
    "authentication"
  ],
  "homepage": "https://github.com/thecybersailor/slauth",
  "license": "Apache-2.0",
  "author": "CYBERSAILOR PTE.LTD.",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./style.css": "./dist/@cybersailor/slauth-ui-vue.css",
    "./dist/@cybersailor/slauth-ui-vue.css": "./dist/@cybersailor/slauth-ui-vue.css",
    "./package.json": "./package.json"
  },
  "workspaces": [
    "../@cybersailor/slauth-ts"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,vue}",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@cybersailor/slauth-ts": "^0.8.1",
    "ua-parser-js": "^1.0.41"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "typescript": "~5.8.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
