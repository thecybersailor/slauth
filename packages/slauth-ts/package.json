{
  "name": "@cybersailor/slauth-ts",
  "version": "0.8.0",
  "description": "Official client library for slauth",
  "keywords": [
    "auth",
    "aira",
    "authentication",
    "typescript"
  ],
  "homepage": "https://github.com/thecybersailor/slauth",
  "license": "Apache-2.0",
  "author": "CYBERSAILOR PTE.LTD.",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "node build.js",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "generate-types": "node ../../tools/generate-types.mjs",
    "generate-types:auth": "swagger-typescript-api generate --path \"$API_DOCS_DIR/auth-api.json\" --output src/types --name auth-api.ts --no-client",
    "generate-types:admin": "swagger-typescript-api generate --path \"$API_DOCS_DIR/admin-api.json\" --output src/types --name admin-api.ts --no-client",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "swagger-typescript-api": "^13.2.13",
    "ts-to-zod": "^4.0.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
